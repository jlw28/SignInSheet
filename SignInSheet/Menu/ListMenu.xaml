             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SignInSheet.Menu"
             mc:Ignorable="d" 
             d:DesignHeight="510.258" d:DesignWidth="945.38">

    <Grid Background="OldLace">
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="319" Margin="125,79,0,0" VerticalAlignment="Top" Width="702" Background="#FFC8C1B3" SelectionChanged="listBox_SelectionChanged"/>
